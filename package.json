{
  "name": "jsx-to-md",
  "version": "0.4.0",
  "private": false,
  "description": "A tool that supports writing Markdown through JSX（一款支持通过 JSX 来编写 Markdown 的工具）",
  "main": "./dist/src/lib/index.js",
  "scripts": {
    "prepare": "husky install",
    "lint-code": "npx lint-staged",
    "lint-msg": "npx commitlint --edit",
    "test": "vitest --ui",
    "publish": "npm publish --access=public --registry https://registry.npmjs.org",
    "dev": "tsc --watch",
    "coverage": "vitest --coverage",
    "docs": "node ./dist/src/bin/index.js run",
    "translate": "i18n t"
  },
  "bin": {
    "jtm": "./dist/src/bin/index.js"
  },
  "files": [
    "./dist/src"
  ],
  "keywords": [
    "md",
    "markdown",
    "jsx"
  ],
  "author": "Eyelly Wu",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix -f table"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@eyelly/prettier-config": "^1.0.0",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitest/coverage-istanbul": "^0.23.4",
    "@vitest/ui": "^0.23.4",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "i18n-pro": "^1.3.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "shelljs": "^0.8.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  }
}
